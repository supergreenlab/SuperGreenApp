<?xml version='1.0' encoding='utf-8'?>
<widget id="com.supergreenlab.app" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="ZeroConf">
        <param name="ios-package" value="ZeroConf" />
    </feature>
    <feature name="BLE">
        <param name="ios-package" onload="true" value="BLECentralPlugin" />
    </feature>
    <name>SuperGreenLab</name>
    <description>
      Control your growbox with your smartphone.
    </description>
    <author email="services@supergreenlab.com" href="http://www.supergreenlab.com">
      Constantin Clauzel
    </author>
    <content src="index.html" />
    <access allows-arbitrary-loads-for-media="true" allows-arbitrary-loads-in-web-content="true" allows-local-networking="true" origin="*" />
    <icon height="1024" src="icon.jpg" width="1024" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="20" src="resources/ios/icon//Icon-20x20@1x.png" width="20" />
    <icon height="40" src="resources/ios/icon//Icon-20x20@2x.png" width="40" />
